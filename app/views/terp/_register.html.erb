




<%= lev_form_for :register, url: terp_register_path(params[:terp_id]) do |f| %>


  <div class='field '>
    <div class='field_label'>Registration Code</div>
    <%= f.text_field :registration_code %>
    <div class='terp_field_help'>Enter registration code as provided by your instructor (case sensitive)</div>
  </div>
  
  <div class='prompt-buttons'><%= f.submit "Register", :class => "ui-state-default link_button" %></div>
<% end %>

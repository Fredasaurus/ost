<%= form_for([@organization, @course]) do |f| %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :typically_offered %><br />
    <%= f.text_field :typically_offered %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
